        android:layout_width="212dp"
        android:layout_height="57dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="11dp"
        android:layout_toRightOf="@+id/imageView1"
        android:layout_x="93dp"
        android:layout_y="14dp"
        android:text="SD NEGERI \tBEKASI SELATAN"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#ffffff"
        android:textSize="22dp"
        android:textStyle="bold"
        android:typeface="serif" />

    <ImageButton
        android:id="@+id/jaka_setia5"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/jaka_mulia2"
        android:layout_below="@+id/jaka_mulia2"
        android:layout_marginTop="13dp"
        android:layout_x="32dp"
        android:layout_y="149dp"
        android:src="@drawable/jaka_setia5" />

    <ImageButton
        android:id="@+id/jakasampurna2"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/jaka_mulia2"
        android:layout_below="@+id/jaka_setia5"
        android:layout_marginTop="14dp"
        android:layout_x="32dp"
        android:layout_y="198dp"
        android:src="@drawable/jakasampurna2" />

    <ImageButton
        android:id="@+id/jaka_mulia2"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_below="@+id/imageView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:src="@drawable/jaka_mulia2" />

    <ImageButton
        android:id="@+id/jati_rasa3"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/jakasampurna2"
        android:layout_below="@+id/jakasampurna2"
