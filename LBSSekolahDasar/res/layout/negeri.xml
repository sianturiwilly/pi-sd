        android:src="@drawable/jati_rasa3" />

    <ImageButton
        android:id="@+id/marga_jaya4"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_above="@+id/pekayon_jaya6"
        android:layout_alignLeft="@+id/jati_asih10"
        android:layout_marginBottom="15dp"
        android:src="@drawable/marga_jaya4" />

    <ImageButton
        android:id="@+id/jati_asih10"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/jati_rasa3"
        android:layout_below="@+id/jati_rasa3"
        android:layout_marginTop="16dp"
        android:src="@drawable/jatih_asih10" />

    <ImageButton
        android:id="@+id/pekayon_jaya6"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/marga_jaya4"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="18dp"
        android:src="@drawable/pekayon_jaya6" />

    <ImageButton
        android:id="@+id/kayuringin_jaya3"
        android:layout_width="280dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/jati_asih10"
        android:layout_below="@+id/jati_asih10"
        android:layout_marginTop="15dp"
        android:src="@drawable/kayuringin_jaya3" />

</RelativeLayout>
